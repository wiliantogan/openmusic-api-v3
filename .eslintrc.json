{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "no-unused-vars": "off",
        "no-undef":"off",
        "max-len":"off"
    }
}